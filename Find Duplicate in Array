int findDuplicate(vector<int> &arr, int n){
	// Write your code here.
    
    int T,d;
    cin>>T;
    int c;
        for(int k=0; k<n; k++)
        {
            d = arr[k];
            c = 0;
        for(int j=0; j<n; j++)
        {
            if(arr[k] == arr[j])
                c++;
}
            if(c > 1)
            return(arr[k]);
}

}
